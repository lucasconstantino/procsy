{
  "name": "procsy",
  "version": "1.0.0",
  "description": "A proxy building helper with subdomain support",
  "keywords": [
    "cli",
    "oclif",
    "proxy",
    "subdomain"
  ],
  "homepage": "https://github.com/lucasconstantino/procsy",
  "bugs": "https://github.com/lucasconstantino/procsy/issues",
  "repository": "lucasconstantino/procsy",
  "license": "MIT",
  "author": "Lucas Constantino Silva @lucasconstantino",
  "files": [
    "/bin",
    "/src"
  ],
  "main": "index.js",
  "bin": {
    "procsy": "./bin/run"
  },
  "scripts": {
    "lint": "eslint .",
    "precommit": "lint-staged",
    "qa": "yarn test && yarn lint",
    "test": "echo NO TESTS"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "eslint-config-standard": "^12.0.0",
    "express": "^4.16.4",
    "express-subdomain": "^1.0.5",
    "http-proxy-middleware": "^0.19.1"
  },
  "devDependencies": {
    "eslint": "^5.5",
    "eslint-config-oclif": "^3.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier-eslint": "^8.8.2",
    "sort-package-json": "^1.19.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "bin": "procsy"
  }
